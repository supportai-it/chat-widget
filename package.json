{
    "name": "@supportai.it/chat-widget",
    "private": false,
    "version": "1.0.5",
    "type": "module",
    "main": "dist/chat-widget.umd.js",
    "module": "dist/chat-widget.es.js",
    "types": "dist/types/main.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/chat-widget.es.js",
            "require": "./dist/chat-widget.umd.js",
            "types": "./dist/types/main.d.ts"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "npm run clean && npm run build:js && npm run build:types",
        "build:js": "vite build",
        "build:types": "tsc --outDir dist/types",
        "clean": "rm -rf dist",
        "preview": "vite preview",
        "prepublishOnly": "npm run build",
        "publish:public": "npm publish --access public"
    },
    "keywords": [
        "supportAI",
        "web-component",
        "chat-widget"
    ],
    "dependencies": {},
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.3",
        "@types/node": "^20.10.0",
        "typescript": "^5.3.2",
        "vite": "^5.0.2",
        "vite-plugin-dts": "^3.6.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supportai-it/chat-widget.git"
    },
    "description": "Chat widget web component for supportAI.it",
    "author": "giacomoarienti",
    "license": "ISC"
}